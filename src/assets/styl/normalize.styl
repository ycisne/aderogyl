@require globalStyl

body, html
    height 100%
    font-family $font-primary

.container
    max-width $max-width
    min-width $min-width

img 
    max-width 100%
    max-height 100% 
    width auto
    height auto

.ad-dodge-color
    mix-blend-mode color-dodge
    position absolute
    width 100%
    height 100%
    z-index 1
